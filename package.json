{
  "name": "eslint-config-trigo",
  "version": "14.0.5",
  "description": "trigo's base JS ESLint config",
  "main": "index.js",
  "repository": "https://github.com/trigo-at/eslint-config-trigo.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.{js,jsx}\"",
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^3",
    "@typescript-eslint/parser": "^3",
    "babel-eslint": "^10",
    "eslint": "^7",
    "eslint-config-airbnb": "^18",
    "eslint-config-airbnb-base": "^14",
    "eslint-plugin-filenames": "^1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^23",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-node": "^11",
    "typescript": "^3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {}
}
