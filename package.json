{
  "name": "eslint-config-trigo",
  "version": "16.0.8",
  "description": "TRIGO ESLint config",
  "main": "node.js",
  "repository": "https://github.com/trigo-at/eslint-config-trigo.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.{js,jsx}\"",
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "_postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "engines": {
    "node": ">=15"
  },
  "peerDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/eslint-plugin": "^7",
    "@babel/preset-react": "^7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7",
    "eslint-config-airbnb": "^18",
    "eslint-config-airbnb-base": "^14",
    "eslint-config-prettier": "^7",
    "eslint-plugin-filenames": "^1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^4"
  },
  "devDependencies": {
    "@babel/core": "7.12.17",
    "@babel/eslint-parser": "7.12.17",
    "@babel/eslint-plugin": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  }
}
