{
  "name": "eslint-config-trigo",
  "version": "12.0.5",
  "description": "trigo's base JS ESLint config",
  "main": "index.js",
  "repository": "https://github.com/trigo-at/eslint-config-trigo.git",
  "author": "trigo GmbH <office@trigo.at>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.{js,jsx}\"",
    "prettify": "prettier --write '**/*.{js,jsx}'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "peerDependencies": {
    "babel-eslint": "^10",
    "eslint": "^6",
    "eslint-config-airbnb": "^18",
    "eslint-config-airbnb-base": "^14",
    "eslint-plugin-filenames": "^1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^23",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {}
}
